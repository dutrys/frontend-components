{
  "name": "frontend-components",
  "type": "module",
  "version": "2.0.39",
  "scripts": {
    "build": "rimraf dist/ && rollup -c && git add dist/",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "push": "npm run build && git add dist/ && git add src/ && git commit -m ",
    "build-tikis-patch": "npm run build && rm -rf ~/Sites/tikis-frontend/node_modules/frontend-components/dist && cp -r ./dist ~/Sites/tikis-frontend/node_modules/frontend-components/",
    "build-schedules-patch": "npm run build && rm -rf ~/Sites/schedules-frontend/node_modules/frontend-components/dist && cp -r ./dist ~/Sites/schedules-frontend/node_modules/frontend-components/",
    "build-tracker-patch": "npm run build && rm -rf ~/Sites/tracker-frontend/node_modules/frontend-components/dist && cp -r ./dist ~/Sites/tracker-frontend/node_modules/frontend-components/",
    "build-cashdesk-patch": "npm run build && rm -rf ~/Sites/cashdesk-frontend/node_modules/frontend-components/dist && cp -r ./dist ~/Sites/cashdesk-frontend/node_modules/frontend-components/",
    "postversion": "git push && git push --tags",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix",
    "lint": "eslint --ext .ts,.tsx src/",
    "prepare": "husky"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@headlessui/react": "*",
    "@heroicons/react": "*",
    "@sentry/nextjs": "*",
    "@tanstack/react-query": "*",
    "classnames": "*",
    "date-fns": "*",
    "date-fns-tz": "*",
    "framer-motion": "*",
    "next-intl": "*",
    "next-nprogress-bar": "^3.0.0",
    "react": "^19.0.0",
    "react-day-picker": "*",
    "react-hook-form": "*",
    "react-hot-toast": "*",
    "react-intersection-observer": "*",
    "react-number-format": "*",
    "react-tooltip": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.35.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-use-client": "^1.4.0",
    "typescript-eslint": "^8.26.1"
  }
}
